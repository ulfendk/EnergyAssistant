{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Error",
      "Microsoft.AspNetCore.Server.Kestrel": "Error"
    },
    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss zzz] ",
      "IncludeScopes": true
    }
